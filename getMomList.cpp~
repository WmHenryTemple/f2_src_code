void getMomList(string angle, string (&arrs)[4], float (&arrf)[4]){
  if(angle=="21")
    {
      arrs[0]="2p7";
      arrs[1]="3p3";
      arrs[2]="4p0";
      arrs[3]="5p1";
      arrf[0]=2.7;
      arrf[1]=3.3;
      arrf[2]=4.0;
      arrf[3]=5.1;
    }

  if(angle=="25")
    {
      arrs[0]="2p7";
      arrs[1]="3p3";
      arrs[2]="4p0";
      arrs[3]="5p1";
      arrf[0]=2.7;
      arrf[1]=3.3;
      arrf[2]=4.0;
      arrf[3]=5.1;
    }

  if(angle=="29")
    {
      arrs[0]="2p7";
      arrs[1]="3p3";
      arrs[2]="4p0";
      arrs[3]="5p1";
      arrf[0]=2.7;
      arrf[1]=3.3;
      arrf[2]=4.0;
      arrf[3]=5.1;
    }

  if(angle=="33")
    {
      arrs[0]="2p7";
      arrs[1]="3p3";
      arrs[2]="4p0";
      arrs[3]="5p1";
      arrf[0]=2.7;
      arrf[1]=3.3;
      arrf[2]=4.0;
      arrf[3]=5.1;
    }

  if(angle=="39")
    {
      arrs[3]="2p5";
      arrs[2]="2p0";
      arrs[1]="1p6";
      arrs[0]="1p3";
      arrf[3]=2.5;
      arrf[2]=2.0;
      arrf[1]=1.6;
      arrf[0]=1.3;
    }
  return;
}
